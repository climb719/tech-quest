<h1>Sign up here: </h1>
<! form for because creating in databade vs login can use form tag not creating or updating in db>
<%= form_for(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :passwordn%>
    <br>

    <%= f.submit %>