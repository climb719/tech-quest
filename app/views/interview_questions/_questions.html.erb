  <% interview_questions.each do |category| %>
    <b><%=category.name  if !category.interview_questions.empty? %></b><br>
    <% category.interview_questions.each do |iq| %>
       <% if @user %>
       <%= link_to iq.question, interview_question_path(iq) if iq.user == current_user %> <br>

        <% else %>
        <%= link_to iq.question, interview_question_path(iq)  %> <br>
  
        <% end %> 
    <% end %>   

<% end %>